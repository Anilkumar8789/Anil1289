class employee():
    def __init__(self,name,age,salary):
        self.name = name
        self.age = age
        self.salary = salary

class childemployee1(employee):
    def __init__(self,name,age,salary):
        self.name = name
        self.age = age
        self.salary = salary

class childemployee2(employee):
    def __init__(self,name,age,salary):
        self.name = name
        self.age = age
        self.salary = salary
emp1 = employee('anil',20,4000)
emp2 = employee('praveen',22,5000)
print(emp1.name)
print(emp1.age)
print(emp1.salary)
print(emp2.name)
print(emp2.age)
print(emp2.salary)
